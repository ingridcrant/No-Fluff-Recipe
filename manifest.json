{
    "name": "No Fluff Recipe",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [ {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      } ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "No Fluff Recipe"
    },
    "permissions": ["tabs", "<all_urls>", "*://*/*"],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}